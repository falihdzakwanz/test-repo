{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "resources/js/**/*",
            "use": "@vercel/static-build",
            "config": { "distDir": "public" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://testtesttest-app.vercel.app/",
        "APP_KEY": "base64:CoVugmNJgA3HuE2brPmrKluesawHa7HRHyvvUA654EE=",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "127.0.0.1",
        "DB_PORT": "3306",
        "DB_DATABASE": "test-db",
        "DB_USERNAME": "root",
        "DB_PASSWORD": "",
        "SESSION_DRIVER": "database",
        "SESSION_LIFETIME": "120",
        "SESSION_ENCRYPT": "false",
        "CACHE_STORE": "database",
        "LOG_CHANNEL": "stack",
